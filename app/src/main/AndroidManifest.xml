<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cardoc.asc1.cardoc" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/redcar"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNewCar"
            android:label="@string/title_activity_add_new_car" >
        </activity>
        <activity
            android:name=".CarInfo"
            android:label="@string/title_activity_car_info" >
        </activity>
        <activity
            android:name=".EditCar"
            android:label="@string/title_activity_edit_car" >
        </activity>
    </application>

</manifest>
